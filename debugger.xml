<application>
  <component name="DebuggerSettings">
    <option name="FORCE_CLASSIC_VM" value="true" />
    <option name="RUN_HOTSWAP_AFTER_COMPILE" value="RunHotswapAlways" />
    <option name="COMPILE_BEFORE_HOTSWAP" value="true" />
  </component>
  <component name="XDebuggerSettings">
    <data-views />
    <general />
    <debuggers>
      <debugger id="groovy_debugger">
        <configuration>
          <option name="DEBUG_DISABLE_SPECIFIC_GROOVY_METHODS" value="false" />
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration doNotStepIntoLibraryCode="false">
          <custom-object-presentation />
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
      <debugger id="kotlin_debugger">
        <configuration>
          <option name="DEBUG_DISABLE_KOTLIN_INTERNAL_CLASSES" value="false" />
          <option name="DEBUG_IS_FILTER_FOR_STDLIB_ALREADY_ADDED" value="true" />
        </configuration>
      </debugger>
      <debugger id="ruby">
        <configuration>
          <option name="multiprocess" value="false" />
          <option name="readNonSuspendedFrames" value="false" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>