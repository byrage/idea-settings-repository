<templateSet group="plain">
  <template name="psf" value="private static final " description="private static final" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="psfi" value="private static final int " description="private static final int" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="psfs" value="private static final String " description="private static final String" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="nm" value="public void $END$() {&#10;}" description="create new method" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="todo" value="// TODO : hero.es $date$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="" defaultValue="date(&quot;Y-MM-d&quot;)" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>