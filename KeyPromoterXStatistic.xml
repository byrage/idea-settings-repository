<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'⌥2'</ShortCut>
          <Description>Favorites</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌥Button1 Click' or '⌥/' or 'F3'</ShortCut>
          <Description>GoTo</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Console">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Open Console</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'ApiServerApplication'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌥⇧X' or '⇧F10' or '⌃F11'</ShortCut>
          <Description>Run 'ApiServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌥6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>